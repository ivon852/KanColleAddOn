          ],
          "on_exit": [
            "/execute @e[type=aircraft:g4m2e, tag=flying] ~ ~ ~ replaceitem entity @s slot.weapon.mainhand 0 air",
            "@s reset_aircraft"
          ]
        },
        "type91torpedo": {
          "transitions": [
            {
              "default": "1.0"
            }
          ],
          "on_entry": ["@s fire_type91torpedo"],
          "on_exit": [
            "/execute @e[type=aircraft:g4m2e, tag=flying] ~ ~ ~ replaceitem entity @s slot.weapon.mainhand 0 air",
