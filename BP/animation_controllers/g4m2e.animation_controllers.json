        },
        "drop_bomb": {
          "transitions": [
            {
              "default": "1.0"
            }
          ],
          "on_entry": [
            "/execute @s ~+5 ~-1 ~ summon tnt",
            "/execute @s ~-5 ~-1 ~ summon tnt",
            "/execute @s ~ ~-1 ~+5 summon tnt",
            "/execute @s ~ ~-1 ~-5 summon tnt",
            "/execute @s ~+5 ~-1 ~+5 summon tnt",
            "/execute @s ~+5 ~-1 ~-5 summon tnt",
