{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.g4m2e.move": {
      "states": {
        "default": {
          "animations": [{ "move": "query.modified_move_speed" }]
        }
      }
    },
    "controller.animation.g4m2e.retract_gear": {
      "states": {
        "default": {
          "animations": [{ "retract_gear": "!query.is_on_ground" }]
        }
      }
    },
    "controller.animation.g4m2e.on_ground": {
      "states": {
        "default": {
          "animations": [{ "on_ground": "query.is_on_ground" }]
        }
      }
    }
  }
}
